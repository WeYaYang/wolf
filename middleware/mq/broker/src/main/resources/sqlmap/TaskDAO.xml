<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapperGateway PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapperGateway.dtd">
<mapperGateway namespace="study.daydayup.wolf.mq.broker.dal.dao.TaskDAO">
    <update id="updateStateById" parameterType="study.daydayup.wolf.mq.broker.dal.dataobject.TaskDO">
        UPDATE wmq_task
        SET
        state = #{state},
        updated_at = #{updated_at}
        WHERE
        id = #{id}
    </update>
</mapperGateway>